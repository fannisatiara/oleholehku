{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle":"off",
        "no-console":"off",
        "import/prefer-default-export": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-param-reassign": "off",
        "consistent-return": "off"
    }
}
